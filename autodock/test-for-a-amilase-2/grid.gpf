npts 40 40 40                        # num.grid points in xyz
gridfld 1hny.maps.fld                # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C Ca Cl HD N OA SA  # receptor atom types
ligand_types C HD OA N               # ligand atom types
receptor 1hny.pdbqt                  # macromolecule
gridcenter 7.88 45.0 19.8            # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map 1hny.C.map                       # atom-specific affinity map
map 1hny.HD.map                      # atom-specific affinity map
map 1hny.OA.map                      # atom-specific affinity map
map 1hny.N.map                       # atom-specific affinity map
elecmap 1hny.e.map                   # electrostatic potential map
dsolvmap 1hny.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
